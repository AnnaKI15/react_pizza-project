items={categoryNames}\n         \n        />\n        <SortPoup\n          activeSortType={sortBy.type}\n          items={sortItems}\n          onClickSortType={onSelectSortType}\n        />\n      </div>\n      <h2 className=\"content__title\">Все пиццы</h2>\n      <div className=\"content__items\">\n        {isLoaded\n          ? items.map((obj) => \n          (<PizzaBlock \n            onClickAddPizza={handelPizzaToCart} \n            key={obj.id} \n             addedCount={cartItems[obj.id] && cartItems[obj.id].items.length}\n             {...obj} /> \n             ))\n          : Array(12)\n              .fill(0)\n              .map((_, index) => <PizzaLoadingBlock key={index} />)}\n      </div>\n    </div>\n  );\n}\n\nexport default Home;","C:\\Users\\User\\Git\\pizza-project\\src\\components\\PizzasBlock\\LoadingBlock.jsx",[],"C:\\Users\\User\\Git\\pizza-project\\src\\components\\PizzasBlock\\index.jsx",[],"C:\\Users\\User\\Git\\pizza-project\\src\\redux\\action\\cart.js",[],"C:\\Users\\User\\Git\\pizza-project\\src\\redux\\action\\pizzas.js",[],"C:\\Users\\User\\Git\\pizza-project\\src\\redux\\action\\filter.js",[],{"ruleId":"90","severity":1,"message":"91","line":26,"column":6,"nodeType":"92","endLine":26,"endColumn":24,"suggestions":"93"},{"ruleId":"90","severity":1,"message":"94","line":30,"column":6,"nodeType":"92","endLine":30,"endColumn":8,"suggestions":"95"},{"ruleId":"90","severity":1,"message":"94","line":34,"column":6,"nodeType":"92","endLine":34,"endColumn":8,"suggestions":"96"},"react-hooks/exhaustive-deps","React Hook React.useEffect has a missing dependency: 'dispatch'. Either include it or remove the dependency array.","ArrayExpression",["97"],"React Hook React.useCallback has a missing dependency: 'dispatch'. Either include it or remove the dependency array.",["98"],["99"],{"desc":"100","fix":"101"},{"desc":"102","fix":"103"},{"desc":"102","fix":"104"},"Update the dependencies array to be: [category, dispatch, sortBy]",{"range":"105","text":"